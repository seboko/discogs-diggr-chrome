{
	"name": "Discogs Diggr",
	"description": "foo",
	"version": "0.1.0",
	"manifest_version": 2,
	"icons": {},
	"background": {
		"scripts": ["./background.js"]
	},
	"options_page": "./options.html",
	"browser_action": {
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"https://www.discogs.com/*"
	],
	"content_scripts": [
        {
        "matches": [
			"https://www.discogs.com/seller/*/profile*"
            ],
        "js": ["./foreground.js"],
        "run_at": "document_end"
        }
    ]
}